{
	"schemaVersion": 1,
	"id": "apex",
	"version": "${version}",
	"name": "APEx",
	"description": "APEx (pronounced \"ape-x\" with emphasis on \"ape\") is A Proper Equipment expansion mod that adds new ores with respective gems and metals, tool types, equipment tiers, and many other equipment-related items.",
	"authors": [
		"ifound1dollar"
	],
	"contact": {
		"homepage": "https://github.com/ifound1dollar",
		"sources": "https://github.com/ifound1dollar/MC_Fabric_APEx"
	},
	"license": "MIT",
	"icon": "assets/apex/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.dollar.apex.ModMain"
		],
		"client": [
			"net.dollar.apex.ModMainClient"
		],
		"fabric-datagen": [
			"net.dollar.apex.ModMainDataGenerator"
		]
	},
	"mixins": [
		"apex.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.7",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}